
<section class="iron--wrap gs-u-clearfix {{classes}}">
  <!-- 5 items group -->

  <div class="gel-wrap">
      {{ molecule "stripenav" }}
  <div class="gel-layout layout--flex@xxl">

    <div class="gel-layout__item">
      <div class="iron--box">
        <div class="gel-layout {{flex-classes}}">
          {{#each promo-items}}
          <div class="spacing--vertical@s spacing--vertical@m spacing--vertical@l spacing--vertical@xxl gel-layout__item iron__item iron__item--{{@index}} {{classes}}">

            {{ molecule "promo"}}

          </div>
          {{/each}}

{{#if promo-groupings}}
          <div class="iron__groupings layout--flex@s {{flex-classes}}">

            {{#each promo-groupings}}
              <div class=" spacing--vertical@l spacing--vertical@xxl gel-layout__item iron__item iron__item--{{@index}} {{classes}}">

                {{ molecule "promo"}}

              </div>
            {{/each}}

          </div>
    {{/if}}
        </div>
        {{#if slice-footer-info}}
        <div class="slice-footer-info gel-brevier">{{slice-footer-info}}</div>
        {{/if}}
      </div>
    </div>

    </div>
  </div>
    <!-- END 5 items group -->
</section>
