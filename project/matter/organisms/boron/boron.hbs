


<section class="boron--wrap gs-u-clearfix gs-u-mt+ {{classes}}">
  <!-- 5 items group -->
  <div class="gel-wrap">
    {{ molecule "stripenav" }}
  <div class="gel-layout">

    <div class="gel-layout__item">
      <div class="boron--box">
        <div class="gel-layout">
          {{#each promo-items}}
          <div class="gel-layout__item boron__item boron__item--{{@index}} {{classes}}">

            {{ molecule "promo"}}

            {{#advert}}
            {{ "molecule" "advert_slot_two"}}
            {{/advert}}

          </div>
          {{/each}}


          <div class="gel-layout__item gel-1/1@m gel-3/4@l gel-1/5@xxl river_1 at-line-b-until@s spacing--vertical@s">
            <div class="boron__river gs-u-mt-@xxl gs-u-mb-@xxl at-line-t-until@m at-line-t-until@l at-line-t-until@xxl">
              <div class="gel-layout gel-layout--flex">
                <!-- Headline 01 -->
                 {{#each promo-headlines}}
                <div class="{{classes}} gel-layout__item boron__item--river river__item--{{@index}} gel-1/3@m gel-1/3@l gel-1/1@xxl">

                  {{ molecule "promo"}}

                </div>
                {{/each}}


                </div>
              </div>
            </div>

          <!--///–––––––––––––––––––––––––––––––––*\
          //    # RIVER
          //\*––––––––––––––––––––––––––––––––––––*/-->

          <div class="gel-layout__item gel-1/1@m gel-3/4@l gel-1/1@xxl river_2">
            <div class="boron__river gs-u-mt-@xxl gs-u-mb-@xxl at-line-t-until@m at-line-t-until@l at-line-t-until@xxl">
              <div class="gel-layout gel-layout--flex">
                <!-- Headline 01 -->
                 {{#each promo-river-items}}
                <div class="{{classes}} gel-layout__item boron__item--river river__item--{{@index}} gel-1/3@m gel-1/3@l gel-1/5@xxl">

                  {{ molecule "promo"}}

                </div>
                {{/each}}


                </div>
              </div>
            </div>

        </div>
      </div>
    </div>



    </div>
    </div>

  </div>
</section>
